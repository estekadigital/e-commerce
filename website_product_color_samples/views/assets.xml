<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="website_product_color_samples.assets_frontend" inherit_id="website.assets_frontend">
            <xpath expr="script[last()]" position="after">
                <script type="text/javascript"
                      src="/website_product_color_samples/static/src/js/color_samples.js"/>
            </xpath>
            <xpath expr="link[last()]" position="after">
                <link rel="stylesheet" type="text/scss"
                      href="/website_product_color_samples/static/src/css/color_samples.scss"/>
            </xpath>
        </template>
    </data>
</odoo>